{
  "name": "@luvies/lazy",
  "version": "1.0.0",
  "description": "A linq-like lazy iteration module that aims to support deno, node & browser",
  "main": "dist/mod.js",
  "repository": "luvies/lazy",
  "author": "luvies",
  "license": "MIT",
  "scripts": {
    "prepare": "rm -rf dist/* && node ./scripts/prebuild.js && tsc --project ./tsconfig.build.json",
    "test": "deno ./test/run_tests.ts && yarn prepare",
    "init-types": "deno --prefetch ./test/run_tests.ts"
  },
  "devDependencies": {
    "deno_ls_plugin": "^0.1.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.3"
  },
  "keywords": [
    "deno",
    "denoland",
    "browser",
    "linq",
    "lazy",
    "enumerable",
    "iteration",
    "iterator",
    "enumerator",
    "enumeration",
    "extensible"
  ]
}
