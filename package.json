{
  "name": "@luvies/lazy",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.7.0",
  "description": "A linq-like lazy iteration module that aims to support deno, node & browser",
  "main": "dist/mod.js",
  "repository": "luvies/lazy",
  "author": "luvies",
  "license": "MIT",
  "scripts": {
    "prepare": "yarn docs",
    "test": "deno run ./test/run_tests.ts",
    "init-types": "deno --prefetch ./test/run_tests.ts",
    "lint": "tslint --project . && prettier --check 'lib/**/*.ts' 'test/**/*.ts'",
    "fix": "tslint --project . --fix && prettier --check --write 'lib/**/*.ts' 'test/**/*.ts'",
    "docs": "yarn build && typedoc --out docs ./prebuild --mode file",
    "postpublish": "git push && git push --tags",
    "build": "rm -rf dist/* && deno run --allow-read --allow-write https://s.luvies.io/node_prebuild.ts --in lib --out prebuild && tsc --project ./tsconfig.build.json"
  },
  "devDependencies": {
    "deno_ls_plugin": "^0.1.0",
    "prettier": "^1.17.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5"
  },
  "keywords": [
    "deno",
    "denoland",
    "browser",
    "linq",
    "lazy",
    "enumerable",
    "iteration",
    "iterator",
    "enumerator",
    "enumeration",
    "extensible"
  ]
}
